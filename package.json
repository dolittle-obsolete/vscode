{
    "name": "@dolittle/vscode",
    "displayName": "Dolittle",
    "description": "Tools and extensions that enables you to become a Dolittle superhero!",
    "version": "0.0.1",
    "author": "Dolittle",
    "publisher": "woksin",

    
  "homepage": "https://dolittle.io",
    "license": "MIT",
    "engines": {
        "vscode": "^1.29.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "bin/extension.js",
    "module": "bin/extension.js",
    "files": [
      "bin"
    ],
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "dolittle",
                    "title": "Dolittle",
                    "icon": "Resources/dolittle_symbol.svg"
                }
            ]
        },
        "views": {
            "dolittle": [
                {
                    "id": "cluster",
                    "name": "Cluster"
                },
                {
                    "id": "eventStore",
                    "name": "Event Store"
                }
            ]
        },
        "commands": [
            {
                "command": "dolittle.build",
                "category": "Dolittle Project",
                "title": "Build"
            },
            {
                "command": "dolittle.buildCurrent",
                "category": "Dolittle Project",
                "title": "Build Current"
            },
            {
                "command": "dolittle.restore",
                "category": "Dolittle Project",
                "title": "Restore"
            },
            {
                "command": "dolittle.test",
                "category": "Dolittle Project",
                "title": "Test"
            },
            {
                "command": "dolittle.testDebug",
                "category": "Dolittle Project",
                "title": "Test Debug"
            },
            {
                "command": "dolittle.testRerun",
                "category": "Dolittle Project",
                "title": "Test Rerun"
            },
            {
                "command": "dolittle.newDolittleProject",
                "category": "Dolittle Project",
                "title": "New Dolittle Project"
            },
            {
                "command": "dolittle.createBoundedContext",
                "category": "Dolittle Project",
                "title": "Create Bounded Context"
            },
            {
                "command": "dolittle.createApplication",
                "category": "Dolittle Project",
                "title": "Create Application"
            },
            {
                "command": "dolittle.reloadProject",
                "category": "Dolittle Project",
                "title": "Reload Projects"
            },
            {
                "command": "dolittle.addArtifact",
                "category": "Dolittle Artifacts",
                "title": "Add"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test",
        "build": "babel Source -d bin --source-maps inline --ignore **/for_*"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.10.25",
        "eslint": "^5.9.0",
        "vscode": "^1.1.21",
        "babel-cli": "^6.26.0",
        "babel-plugin-transform-async-generator-functions": "^6.24.1",
        "babel-plugin-transform-async-to-generator": "^6.24.1",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-decorators-legacy": "^1.3.5",
        "babel-plugin-transform-flow-strip-types": "^6.22.0",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-stage-1": "^6.24.1"

    },
    "dependencies": {
        "@dolittle/cli": "^2.0.0-alpha2.59",
        "babel-runtime": "^6.26.0",
        "fs-extra": "^7.0.0",
        "shelljs": "^0.8.2"
    }
}
